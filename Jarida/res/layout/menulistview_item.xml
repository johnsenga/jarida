<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:layout_marginBottom="15dp"
    android:layout_marginTop="15dp"
    android:orientation="horizontal" >
	
    
    
    
    <ImageView
        android:id="@+id/imgIcon"
        android:layout_width="30dp"
        android:layout_height="30dp"
        android:padding="5dp"
        android:layout_margin="5dp"
        android:src="@drawable/thestart" />

    
    <com.devspark.robototextview.widget.RobotoTextView
        android:id="@+id/textViewIcon"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="10dp"
        android:layout_alignBaseline="@+id/tvLinkToRegisterScreen"
        android:layout_alignBottom="@+id/tvLinkToRegisterScreen"
        android:layout_toLeftOf="@+id/tvLinkToRegisterScreen"
        android:text="Placeholder"
        android:textSize="18sp"
        app:typeface="roboto_slab_thin"
        android:textColor="@color/purple"
        />
    
    


</LinearLayout>